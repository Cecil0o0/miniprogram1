.index
  padding 16px 30px
  .search_input
    height 80px
    background-color #EEEEEE
    display flex
    justify-content flex-start
    align-items center
    border-radius 40px
    padding 0 38px
    margin-bottom 16px
    text
      color #000
      font-size 26px
      margin-left 20px
  .swiper
    swiper-item
      view
        height 100%
        background-size 100% 100%
        border-radius 20px
        image
          width 100%
          height 100%
          border-radius 20px
  .model-cards-wrapper
    display flex
    flex-flow row wrap
    justify-items space-between
    justify-content space-between
    model-card
      margin-bottom 46px
