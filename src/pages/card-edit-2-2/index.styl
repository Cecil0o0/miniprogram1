.page-edit-2-2
  display flex
  .basic-info
    width 103px
    height 317px
    display flex
    justify-content space-between
    align-items center
    flex-direction column
    .name
      font-size 16px
      color #000
      margin-top 14px
    .label,
    .value
      font-size 11px
      color #000
      line-height 1
      margin-bottom 4px
    .qrcode
      width 80px
      height 80px
      font-size 11px
      background-color #000000
      border-radius 8px
  .imgs-wrapper
    flex 1
    position relative
    >view
      position absolute
      background-color #999
    .img1,
    .img6
      left 0
      top 2px
      width 214px
      height 313px
    .img2,
    .img3,
    .img4,
    .img5
      width 107px
      height 155px
    .img2,
    .img3
      height 156px
    .img2
      top 2px
      left 216px
    .img3
      top 2px
      left 325px
    .img4
      bottom 2px
      left 216px
    .img5
      left 325px
      bottom 2px
    .img6
      left auto
      right 2px
      top 2px
